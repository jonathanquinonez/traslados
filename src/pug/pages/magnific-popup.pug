extends ../layout/defualt-layout.pug
block title
	title Forge :: Magnific Gallery Popup
block append css
	link(rel="stylesheet", href="../css/plugins/magnific-popup/magnific-popup.css")
	
block content
	- var mainRoute="gallery"
	- var route = 'gallery'
	- var func = 'magnificPopup'
