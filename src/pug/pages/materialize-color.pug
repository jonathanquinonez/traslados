extends ../layout/defualt-layout.pug
block title
	title Forge :: Materialize Color
block append css
	
block content
	- var mainRoute="appearance"
	- var route = 'materializeColor'
	- var func = ''
	- var nestedName=""
	- var breadcrumb=[{"link":"index.html","title":"Home"},{"link":"#","title":"Color"}]
	- var pageGeneralDropDown=[{"type":"normal","link":'#',"icon":"account_box","iType":"material","title":"Account"},{"type":"normal","link":'#',"icon":"local_activity","iType":"material","title":"Recent Activities"},{"type":"normal","link":'#',"icon":"mdi-wheelchair-accessibility","iType":"mdi","title":"Accessibility"},{"type":"divider"},{"type":"normal","link":'#',"icon":"settings","iType":"material","title":"All Settings"}]
	include ../include/header.pug
	main.animated
		+dropdownContent('generalDropDown',pageGeneralDropDown)
		// ============================
		// MAIN HEADER
		// ============================
		.main-header
			.sec-page
				.page-title
					h2 Materialize Color Scheme
				.page-options
					a(href="#",class="dropdown-button page-opt-dropBtn btn-floating waves-effect waves-set setWave",data-activates='generalDropDown',data-beloworigin="true",data-alignment="right",data-position="bottom",data-constrainwidth="false",data-delay="50",data-gutter="25")
						+mi('perm_data_setting')
					a(href="#",class="waves-effect waves-set page-opt-dropBtn setWave btn-floating")
						+mi('chat_bubble_outline')
			// ============================
			// breadcrumb
			// ============================
			.sec-breadcrumb
				+breadcrumb(breadcrumb,'left')
		// ============================
		// Main container
		// ============================
		.main-container
			//- Tags
			div.row
				.col.s12
					h4.content-headline Color
					p
						|Here is a color palette based on the material design base colors. Each of these colors is defined with a base color class and an optional lighten or darken class.
				// Background Color
				.col.s12
					.card-panel
						.row.box-title
							div.col.s12
								h5.content-headline Background Color
								p To apply a background color, just add the color name and light/darkness as a class to the element.
						.row
							div.col.s12
								.card-panel.primary-bg.light.white-text
									| This is a card panel with a teal lighten-2 class
								pre.language-markup.
									<code class="  language-markup">
									<span class="token tag"><span class="token tag"><span class="token punctuation">&lt;</span>div</span> <span class="token attr-name">class</span><span class="token attr-value"><span class="token punctuation">=</span><span class="token punctuation">"</span>card-panel teal lighten-2<span class="token punctuation">"</span></span><span class="token punctuation">&gt;</span></span>This is a card panel with a teal lighten-2 class<span class="token tag"><span class="token tag"><span class="token punctuation">&lt;/</span>div</span><span class="token punctuation">&gt;</span></span>
									</code>
				// Text COlor
				.col.s12
					.card-panel
						.row.box-title
							div.col.s12
								h5.content-headline Text Color
								p To apply a text color, just append -text to the color class like this:
						.row
							div.col.s12
								.card-panel
									.primary-text
										| This is a card panel with dark blue text
								pre.language-markup.
									<code class="language-markup">
									<span class="token tag"><span class="token tag"><span class="token punctuation">&lt;</span>div</span> <span class="token attr-name">class</span><span class="token attr-value"><span class="token punctuation">=</span><span class="token punctuation">"</span>card-panel<span class="token punctuation">"</span></span><span class="token punctuation">&gt;</span></span>
									  <span class="token tag"><span class="token tag"><span class="token punctuation">&lt;</span>span</span> <span class="token attr-name">class</span><span class="token attr-value"><span class="token punctuation">=</span><span class="token punctuation">"</span>blue-text text-darken-2<span class="token punctuation">"</span></span><span class="token punctuation">&gt;</span></span>This is a card panel with dark blue text<span class="token tag"><span class="token tag"><span class="token punctuation">&lt;/</span>span</span><span class="token punctuation">&gt;</span></span>
									<span class="token tag"><span class="token tag"><span class="token punctuation">&lt;/</span>div</span><span class="token punctuation">&gt;</span></span></code>
				// Sass
				.col.s12
					.card-panel
						.row.box-title
							div.col.s12
								h5.content-headline Sass
						.row
							div.col.s12
								p For background colors, you can apply the color simply by extending the classes like the example below.
								pre.language-sass.
									<code class=" language-sass">
									<span class="token selector">.ilike-blue-container {</span>
									<span class="token atrule-line">    <span class="token atrule">@extend</span> .blue, .lighten-4;</span>
									<span class="token selector">}</span></code>
							div.col.s12
								p For changing text color, you can apply the color simply by extending the classes like the example below.
								pre.language-sass.
									<code class="  language-sass">
									<span class="token selector">.ilike-blue-container {</span>
									<span class="token atrule-line">	<span class="token atrule">@extend</span> .blue-text, .text-lighten-4;</span>
									<span class="token selector">}</span></code>
