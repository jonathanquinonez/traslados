extends ../layout/defualt-layout.pug
block title
	title Forge :: Form Extended Control
block append css
	
	link(rel="stylesheet", href="../plugins/jquery.numbox-1.2.0/jquery.numbox-1.2.0.css")
	link(rel="stylesheet", href="../plugins/intl-tel-input/build/css/intlTelInput.css")
block content
	- var mainRoute="UIElements"
	- var route = 'formComponets'
	- var func = 'extendedControl'
	- var breadcrumb=[{"link":"index.html","title":"Home"},{"link":"#","title":"Form Componets"},{"link":"#","title":"Extended Control"}]
	- var pageGeneralDropDown=[{"type":"normal","link":'#',"icon":"account_box","iType":"material","title":"Account"},{"type":"normal","link":'#',"icon":"local_activity","iType":"material","title":"Recent Activities"},{"type":"normal","link":'#',"icon":"mdi-wheelchair-accessibility","iType":"mdi","title":"Accessibility"},{"type":"divider"},{"type":"normal","link":'#',"icon":"settings","iType":"material","title":"All Settings"}]
	include ../include/header.pug
	main.animated
		+dropdownContent('generalDropDown',pageGeneralDropDown)
		// ============================
		// MAIN HEADER
		// ============================
		.main-header
			.sec-page
				.page-title
					h2 Extended Control
				.page-options
					a(href="#",class="dropdown-button page-opt-dropBtn btn-floating waves-effect waves-set setWave",data-activates='generalDropDown',data-beloworigin="true",data-alignment="right",data-position="bottom",data-constrainwidth="false",data-delay="50",data-gutter="25")
						+mi('perm_data_setting')
					a(href="#",class="waves-effect waves-set page-opt-dropBtn setWave btn-floating")
						+mi('chat_bubble_outline')
			// ============================
			// breadcrumb
			// ============================
			.sec-breadcrumb
				+breadcrumb(breadcrumb,'left')
		// ============================
		// Main container
		// ============================
		.main-container
			//- Masked Input
			div.row
				.col.s12
					h4.content-headline Extended Control
					p 
						| Extended controls includes very handly input types, which extend you input paramters. These will expand your choice of input parameter while you develope an application.
				// Masked Input
				form.col.s12
					.card-panel
						.row.box-title
							div.col.s12
								h5.content-headline Masked Input
								p
									|<code class="language-markup">jquery.maskedinput.js</code> is being used for this extended control over the input. Add <code class="language-markup">masked-input/jquery.maskedinput.js</code> to your page and design your requirement to be performed. It's so easy.
						.row
							.input-field.col.s12.m6
								input(type="text",id="masked-date1")
								label(for="masked-date1") Date
							.input-field.col.s12.m6
								input(type="text",id="masked-phone",class='phone')
								label(for="masked-phone") Phone
						
							.input-field.col.s12.m6
								input(type="text",id="masked-phoneExt")
								label(for="masked-phoneExt") PhoneExt
							.input-field.col.s12.m6
								input(type="text",id="masked-iphone")
								label(for="masked-iphone") iPhone
						
							.input-field.col.s12.m6
								input(type="text",id="masked-tin")
