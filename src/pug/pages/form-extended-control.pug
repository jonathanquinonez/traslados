							.input-field.col.s12.m6
								input(type="text",id="masked-eyescript")
								label(for="masked-eyescript") eyescriptpo
						
							.input-field.col.s12.m6
								input(type="text",id="masked-po")
								label(for="masked-po") po
							.input-field.col.s12.m6
