	script(type="text/javascript",src="../plugins/pickers/lolliclock/lolliclock.js")
	script(type="text/javascript",src="../plugins/pickers/jquery-timepicker/jquery.timepicker.js")
	script.
		$(document).ready(function(){
			$('.datepicker').pickadate({
				selectMonths: true, // Creates a dropdown to control month
				selectYears: 15 // Creates a dropdown of 15 years to control year
			});
			//- lolliclock
			$("#clock1").lolliclock({autoclose:true});

			$("#clock2").lolliclock({
				autoclose:true,
				schemeActBtn:'secondary-bg',
				schemePopover:'primary-bg',
				schemeHead:'primary-bg dark',
				schemePlat:'white'
			});
			$("#clock3").lolliclock({
				autoclose:true,
				schemeActBtn:'primary-bg',
				schemePopover:'secondary-bg',
				schemeHead:'secondary-bg dark',
				schemePlat:'white'
			});
			$("#clock4").lolliclock({
				autoclose:true,
				schemeActBtn:'error-bg',
				schemePopover:'info-bg',
				schemeHead:'info-bg dark',
				schemePlat:'white'
			});
			//- jQuery TimePicker
			$('#jq-timepick-basicExample').timepicker();
			$('#scrollDefaultExample').timepicker({ 'scrollDefault': 'now' });
			$('#setTimeExample').timepicker();
			$('#setTimeButton').on('click', function (){
			    $('#setTimeExample').timepicker('setTime', new Date());
			});
			$('#durationExample').timepicker({
			    'minTime': '2:00pm',
			    'maxTime': '11:30pm',
			    'showDuration': true
			});

			$('#onselectExample').timepicker();
			$('#onselectExample').on('changeTime', function() {
			    $('#onselectTarget').text($(this).val());
			});
			$('#disableTimeRangesExample').timepicker({
			    'disableTimeRanges': [
			        ['1am', '2am'],
			        ['3am', '4:01am']
			    ]
			});
			$('#noneOptionExample').timepicker({
			    'noneOption': [
			        {
			            'label': 'Foobar',
			            'className': 'shibby',
			            'value': '42'
			        },
			        'Foobar2'
			    ]
			});

			$('#timeformatExample1').timepicker({ 'timeFormat': 'H:i:s' });
			$('#timeformatExample2').timepicker({ 'timeFormat': 'h:i A' });

			$('#stepExample1').timepicker({ 'step': 15 });
			$('#stepExample2').timepicker({
			    'step': function(i) {
			        return (i%2) ? 15 : 45;
			    }
			});

		});